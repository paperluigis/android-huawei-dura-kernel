		slew-rate = <1>;
		output-high;
		};
	};
	flashlights_aw3643_pins_hwen_high: hwen_high{
	pins_cmd_dat{
		pins = <PINMUX_GPIO10__FUNC_GPIO10>;
		slew-rate = <1>;
		output-high;
		};
	};
	flashlights_aw3643_pins_hwen_low: hwen_low{
	pins_cmd_dat{
		pins = <PINMUX_GPIO10__FUNC_GPIO10>;
		slew-rate = <1>;
		output-low;
		};
	};
	flashlights_aw3643_pins_strobe_high: strobe_high{
	pins_cmd_dat{
		pins = <PINMUX_GPIO3__FUNC_GPIO3>;
